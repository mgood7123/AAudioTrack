<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="sequencer">

        <attr name="channels" format="integer" />
        <attr name="channelHeight" format="dimension" />
        <attr name="fitChannelsToView" format="boolean" />

        <!-- the resolution of the note grid in the native api -->
        <attr name="nativeNotes" format="integer" />

        <!-- the resolution of the note grid in the UI -->
        <attr name="viewNotes" format="integer" />

        <attr name="noteWidth" format="dimension" />
        <attr name="fitNotesToView" format="boolean" />
    </declare-styleable>

</resources>